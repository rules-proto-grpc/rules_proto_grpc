bazel_dep(name = "another_workspace", version = "0.0.0")
bazel_dep(name = "protobuf", version = "30.2")
bazel_dep(name = "rules_proto_grpc_python", version = "0.0.0.rpg.version.placeholder")
bazel_dep(name = "rules_python", version = "1.4.1")

local_path_override(
    module_name = "another_workspace",
    path = "another_workspace",
)

local_path_override(
    module_name = "rules_proto_grpc",
    path = "../../modules/core",
)

local_path_override(
    module_name = "rules_proto_grpc_python",
    path = "../../modules/python",
)

python = use_extension("@rules_python//python/extensions:python.bzl", "python")
python.toolchain(python_version = "3.11")
